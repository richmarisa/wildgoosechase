{
    "name": "cubear/drupal-8-starter-kit",
    "description": "Project template for Drupal 8 projects with composer, also Pantheon; from Cornell University CIT-CD.",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Alison McCauley (alisonjo2786)",
            "homepage": "https://www.drupal.org/u/alisonjo2786",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
          "type": "package",
          "package": {
            "name": "jackmoore/colorbox",
            "version": "1.6.4",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
              "type": "zip"
             }
          }
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.7.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.7.0/chosen_v1.7.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.0.20",
        "cubear/cwd_admin": "1.0.0-alpha1",
        "cubear/cwd_base_bootstrap": "1.0.0-alpha2",
        "cweagans/composer-patches": "^1.0",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "drupal/admin_toolbar": "^1.19",
        "drupal/admin_toolbar_tools": "^1.19",
        "drupal/better_field_descriptions": "^1.2",
        "drupal/bootstrap": "3.x-dev",
        "drupal/bootstrap_paragraphs": "^2.0",
        "drupal/chosen": "^2.4",
        "drupal/chosen_field": "^2.4",
        "drupal/chosen_lib": "^2.4",
        "drupal/ckeditor_media_embed": "^1.3",
        "drupal/coffee": "1.0-beta2",
        "drupal/collapse_text": "^1.0",
        "drupal/colorbox": "^1.4",
        "drupal/config_direct_save": "^1.0",
        "drupal/config_installer": "^1.0",
        "drupal/config_update": "^1.3",
        "drupal/config_update_ui": "^1.3",
        "drupal/console": "^1",
        "drupal/core": "^8",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.0",
        "drupal/diff": "1.0-rc1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity": "1.0-beta3",
        "drupal/entity_browser": "^1.1",
        "drupal/entity_embed": "^1.0",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/exif_orientation": "^1.0",
        "drupal/externalauth": "^1.0",
        "drupal/features": "^3.5",
        "drupal/features_ui": "^3.5",
        "drupal/field_group": "1.0.0",
        "drupal/google_analytics": "^2.2",
        "drupal/inline_entity_form": "^1.0",
        "drupal/linkit": "^5.0@beta",
        "drupal/media_entity_actions": "^1.0",
        "drupal/media_entity_browser": "^2.0",
        "drupal/media_entity_generic": "^1.0",
        "drupal/media_entity_instagram": "^2.0",
        "drupal/media_entity_twitter": "^2.0",
        "drupal/metatag": "^1.5",
        "drupal/module_filter": "3.1.0",
        "drupal/override_node_options": "^2.1",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.2",
        "drupal/redis": "^1.0.0-rc2",
        "drupal/simplesamlphp_auth": "3.0.0",
        "drupal/taxonomy_image": "^1.0",
        "drupal/token": "^1.0",
        "drupal/twig_tweak": "^1.8",
        "drupal/vertical_tabs_config": "^1.2",
        "drupal/video_embed_field": "^2.0",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "~8",
        "harvesthq/chosen": "^1.7",
        "jackmoore/colorbox": "^1.6",
        "rvtraveller/qs-composer-installer": "^1.1",
        "symfony/console": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "webflo/drupal-core-strict": "8.5.1",
        "webflo/drupal-finder": "^0|^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.2",
        "behat/behat": "3.*",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "jcalderonzumba/gastonjs": "^1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "drupal/drupal-extension": "^3.1",
        "drush-ops/behat-drush-endpoint": "^0.0.4",
        "pantheon-systems/quicksilver-pushback": "~1",
        "phpunit/phpunit": "^4.8",
        "symfony/css-selector": "^2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": "echo 'No code sniff step defined.'",
        "unit-test": "echo 'No unit test step defined.'",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
        "post-install-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "build-env": {
            "install-cms": [
                "drush site-install config_installer --account-mail={account-mail} --account-name={account-name} --site-mail={site-mail} --site-name={site-name} --yes"
            ],
            "export-configuration": "drush config-export --yes"
        },
        "patches-file": "composer.patches.json",
        "drupal-scaffold": {
            "source": "https://raw.githubusercontent.com/pantheon-systems/drops-8/{version}/{path}",
            "includes": [
                "sites/default/default.services.pantheon.preproduction.yml",
                "sites/default/settings.pantheon.php"
            ],
            "excludes": [
                ".csslintrc",
                ".eslintignore",
                ".eslintrc.json",
                "web.config"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.0.8"
        }
    }
}
